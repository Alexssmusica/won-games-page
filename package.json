{
	"name": "won-games-page",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint src --max-warnings=0",
		"typechecking": "tsc --project tsconfig.json --noEmit",
		"prettier": "prettier src/**/*.{tsx,js,ts} --write",
		"test": "jest",
		"test:watch": "yarn test --watch",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"test:e2e": "start-server-and-test dev 3000 cy:open",
		"schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
		"schema:convert": "node convertToNonNull.js",
		"codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
		"types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"generate": "yarn plop --plopfile ./generators/plopfile.js",
		"prepare": "npx husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*": [
			"yarn test --findRelatedTests --bail"
		]
	},
	"dependencies": {
		"@apollo/client": "^3.5.10",
		"@stripe/react-stripe-js": "^1.7.0",
		"@stripe/stripe-js": "^1.25.0",
		"@styled-icons/boxicons-regular": "^10.38.0",
		"@styled-icons/boxicons-solid": "^10.38.0",
		"@styled-icons/fa-brands": "^10.38.0",
		"@styled-icons/material-outlined": "^10.34.0",
		"@styled-icons/remix-fill": "^10.18.0",
		"date-fns": "^2.27.0",
		"graphql": "^15.8.0",
		"joi": "^17.6.0",
		"lodash.xor": "^4.5.0",
		"next": "12.1.1",
		"next-auth": "3.29.0",
		"next-pwa": "^5.4.6",
		"next-seo": "^5.2.0",
		"nextjs-progressbar": "^0.0.14",
		"polished": "^4.1.4",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-slick": "^0.28.1",
		"styled-components": "^5.3.5",
		"styled-media-query": "^2.1.2"
	},
	"devDependencies": {
		"@jackfranklin/test-data-bot": "^2.0.0",
		"@next/eslint-plugin-next": "^12.1.0",
		"@storybook/addon-essentials": "^6.4.19",
		"@storybook/builder-webpack5": "^6.4.19",
		"@storybook/manager-webpack5": "^6.4.19",
		"@storybook/react": "^6.4.19",
		"@testing-library/cypress": "^8.0.2",
		"@testing-library/jest-dom": "^5.16.3",
		"@testing-library/react": "^12.1.4",
		"@testing-library/react-hooks": "^7.0.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.4.1",
		"@types/lodash.xor": "^4.5.6",
		"@types/node": "^17.0.23",
		"@types/react": "^17.0.43",
		"@types/react-slick": "^0.23.8",
		"@types/styled-components": "^5.1.24",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"apollo": "^2.33.9",
		"cypress": "^9.5.2",
		"cypress-plugin-stripe-elements": "^1.0.2",
		"dotenv": "^16.0.0",
		"eslint": "^8.12.0",
		"eslint-config-next": "^12.1.2",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-storybook": "^0.5.7",
		"husky": "^7.0.4",
		"jest": "^27.5.1",
		"jest-styled-components": "^7.0.8",
		"lint-staged": "^12.3.7",
		"msw": "^0.39.2",
		"node-fetch": "2.6.6",
		"plop": "^3.0.5",
		"prettier": "2.6.1",
		"start-server-and-test": "^1.14.0",
		"storybook-addon-next-router": "^3.1.1",
		"typescript": "^4.6.3",
		"webpack": "5"
	}
}
