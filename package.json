{
	"name": "won-games-page",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint src --max-warnings=0",
		"typechecking": "tsc --project tsconfig.json --noEmit",
		"prettier": "prettier src/**/*.{tsx,js,ts} --write",
		"test": "jest",
		"test:watch": "yarn test --watch",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"test:e2e": "start-server-and-test dev 3000 cy:open",
		"schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
		"schema:convert": "node convertToNonNull.js",
		"codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
		"types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"generate": "yarn plop --plopfile ./generators/plopfile.js",
		"prepare": "npx husky install"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*": [
			"yarn test --findRelatedTests --bail"
		]
	},
	"dependencies": {
		"@apollo/client": "^3.8.7",
		"@stripe/react-stripe-js": "^2.3.1",
		"@stripe/stripe-js": "^2.1.11",
		"@styled-icons/boxicons-regular": "^10.47.0",
		"@styled-icons/boxicons-solid": "^10.47.0",
		"@styled-icons/fa-brands": "^10.47.0",
		"@styled-icons/material-outlined": "^10.47.0",
		"@styled-icons/remix-fill": "^10.46.0",
		"date-fns": "^2.30.0",
		"graphql": "^16.8.1",
		"joi": "^17.11.0",
		"lodash.xor": "^4.5.0",
		"next": "14.0.1",
		"next-auth": "3.29.0",
		"next-pwa": "^5.6.0",
		"next-seo": "^6.4.0",
		"nextjs-progressbar": "^0.0.16",
		"polished": "^4.2.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-slick": "^0.29.0",
		"styled-components": "^6.1.0",
		"styled-media-query": "^2.1.2"
	},
	"devDependencies": {
		"@jackfranklin/test-data-bot": "^2.1.0",
		"@next/eslint-plugin-next": "^14.0.1",
		"@storybook/addon-essentials": "^7.5.3",
		"@storybook/builder-webpack5": "^7.5.3",
		"@storybook/manager-webpack5": "^6.5.16",
		"@storybook/react": "^7.5.3",
		"@testing-library/cypress": "^10.0.1",
		"@testing-library/jest-dom": "^6.1.4",
		"@testing-library/react": "^14.0.0",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.5.1",
		"@types/jest": "^29.5.7",
		"@types/lodash.xor": "^4.5.8",
		"@types/node": "^20.8.10",
		"@types/react": "^18.2.37",
		"@types/react-slick": "^0.23.11",
		"@types/styled-components": "^5.1.29",
		"@typescript-eslint/eslint-plugin": "^6.10.0",
		"@typescript-eslint/parser": "^6.10.0",
		"apollo": "^2.34.0",
		"cypress": "^13.4.0",
		"cypress-plugin-stripe-elements": "^1.0.2",
		"dotenv": "^16.3.1",
		"eslint": "^8.53.0",
		"eslint-config-next": "^14.0.1",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-cypress": "^2.15.1",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-storybook": "^0.6.15",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"jest-styled-components": "^7.2.0",
		"lint-staged": "^15.1.0",
		"msw": "^2.0.5",
		"node-fetch": "2.6.6",
		"plop": "^4.0.0",
		"prettier": "3.0.3",
		"start-server-and-test": "^2.0.2",
		"storybook-addon-next-router": "^4.0.2",
		"typescript": "^5.2.2",
		"webpack": "5"
	}
}
