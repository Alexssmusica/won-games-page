{
    "name": "won-games-page",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint src --max-warnings=0",
        "typechecking": "tsc --project tsconfig.json --noEmit",
        "prettier": "prettier src/**/*.{tsx,js,ts} --write",
        "test": "jest",
        "test:watch": "yarn test --watch",
        "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
        "schema:convert": "node convertToNonNull.js",
        "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
        "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
        "storybook": "start-storybook -s ./public -p 6006",
        "build-storybook": "build-storybook -s ./public",
        "generate": "yarn plop --plopfile ./generators/plopfile.js",
        "prepare": "npx husky install"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*": [
            "yarn lint --fix",
            "yarn test --findRelatedTests --bail"
        ]
    },
    "dependencies": {
        "@apollo/client": "^3.5.5",
        "@styled-icons/boxicons-regular": "^10.38.0",
        "@styled-icons/boxicons-solid": "^10.38.0",
        "@styled-icons/fa-brands": "^10.38.0",
        "@styled-icons/material-outlined": "^10.34.0",
        "@styled-icons/remix-fill": "^10.18.0",
        "date-fns": "^2.26.0",
        "graphql": "^16.0.1",
        "lodash.xor": "^4.5.0",
        "next": "12.0.4",
        "next-pwa": "^5.4.1",
        "polished": "^4.1.3",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-slick": "^0.28.1",
        "styled-components": "^5.3.3",
        "styled-media-query": "^2.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "@babel/preset-typescript": "^7.16.0",
        "@next/eslint-plugin-next": "^12.0.3",
        "@storybook/addon-essentials": "^6.3.12",
        "@storybook/builder-webpack5": "^6.3.12",
        "@storybook/manager-webpack5": "^6.3.12",
        "@storybook/react": "^6.3.12",
        "@testing-library/jest-dom": "^5.15.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/react-hooks": "^7.0.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.0.3",
        "@types/lodash.xor": "^4.5.6",
        "@types/node": "^16.11.9",
        "@types/react": "^17.0.36",
        "@types/react-slick": "^0.23.7",
        "@types/styled-components": "^5.1.14",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "apollo": "^2.33.9",
        "babel-jest": "^27.3.1",
        "babel-loader": "^8.2.3",
        "babel-plugin-styled-components": "^2.0.1",
        "eslint": "^7.32.0",
        "eslint-config-next": "^12.0.4",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "husky": "^7.0.4",
        "jest": "^27.3.1",
        "jest-styled-components": "^7.0.8",
        "lint-staged": "^12.1.2",
        "plop": "^2.7.6",
        "prettier": "2.4.1",
        "typescript": "^4.5.2",
        "webpack": "^5.64.2"
    }
}
